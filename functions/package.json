{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "build": "./node_modules/.bin/tsc && cp serviceAccountKey.json lib/",
    "lint": "tslint .",
    "serve": "firebase serve --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "mocha \"lib/tests/**/*.test.js\" --require mocha-steps",
    "coverage": "nyc mocha \"lib/tests/**/*.test.js\" --require mocha-steps"
  },
  "engines": {
    "node": "8"
  },
  "main": "lib/index.js",
